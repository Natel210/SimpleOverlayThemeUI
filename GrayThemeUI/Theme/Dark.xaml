<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="GrayThemeUI.Theme.Foreground" Color="#DADADA"/>
    <SolidColorBrush x:Key="GrayThemeUI.Theme.Background" Color="#252525"/>

    <SolidColorBrush x:Key="GrayThemeUI.Theme.Foreground.Disable" Color="#A0808080"/>

    <SolidColorBrush x:Key="GrayThemeUI.Overlay.BorderBrush.Disable" Color="#25808080"/>
    <SolidColorBrush x:Key="GrayThemeUI.Overlay.BorderBrush.Default" Color="#33EBEBEB"/>
    <SolidColorBrush x:Key="GrayThemeUI.Overlay.BorderBrush.MouseOver" Color="#80EBEBEB"/>
    <SolidColorBrush x:Key="GrayThemeUI.Overlay.BorderBrush.Focus" Color="#EBEBEB"/>

    <SolidColorBrush x:Key="GrayThemeUI.Overlay.Background.Disable" Color="#05808080"/>
    <SolidColorBrush x:Key="GrayThemeUI.Overlay.Background.Default" Color="#10808080"/>
    <SolidColorBrush x:Key="GrayThemeUI.Overlay.Background.MouseOver" Color="#25808080"/>
    <SolidColorBrush x:Key="GrayThemeUI.Overlay.Background.Focus" Color="#40808080"/>

</ResourceDictionary>