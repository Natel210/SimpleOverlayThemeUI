<UserControl x:Class="SimpleOverlayThemeExample.Example.GroupBoxExample"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SimpleOverlayThemeExample.Example"
             mc:Ignorable="d" 
             Style="{StaticResource SimpleOverlayTheme.UserControl.Default}"
             d:Height="{StaticResource exampleHeight}" d:DesignWidth="800">
    <Grid>
        <GroupBox Header="Group Box" Style="{StaticResource SimpleOverlayTheme.GroupBox.Default}" Margin="5">
            <Border Width="500" Height="500" Style="{StaticResource SimpleOverlayTheme.Border.Empty}"/>


        </GroupBox>
    </Grid>
</UserControl>
