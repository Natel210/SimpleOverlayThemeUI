<UserControl x:Class="SimpleOverlayThemeExample.Example.LabelExample"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SimpleOverlayThemeExample.Example"
             mc:Ignorable="d" 
             Style="{StaticResource SimpleOverlayTheme.UserControl.Default}"
             d:DesignWidth="800"
             d:Height="{StaticResource exampleHeight}">
    <StackPanel Orientation="Vertical">
        <Label Content="Lebel" Style="{StaticResource SimpleOverlayTheme.Label.Default}"/>
        <Label Content="Lebel Separator" Style="{StaticResource SimpleOverlayTheme.Label.Separator}"/>
    </StackPanel>
</UserControl>
